CC=gcc
CFLAGS=-O2 -m486 -If:/st7/include

OBJS= bmp.obj gpi.obj

%.obj: %.c
	$(CC) $(CFLAGS) -S $<

        gcc $(CFLAGS) $(OBJS) -o >NUL

bmp.obj: bmp.c
gpi.obj: gpi.c

clean:
	del *.obj
