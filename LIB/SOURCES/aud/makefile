CC=gcc
CFLAGS=-O2 -m486 -If:/st7/include

OBJS= app.obj low.obj

%.obj: %.c
	$(CC) $(CFLAGS) -c $<

        gcc $(CFLAGS) $(OBJS) -o >NUL

app.obj: app.S
low.obj: low.S

clean:
	del *.obj
